{
  "name": "globalizer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng  update --all",
    "serve": "ng serve",
    "build:prod": "ng build --prod --configuration='production' --progress=false",
    "test": "ng test",
    "lint": "ng lint",
    "start": "node server/index.js",
    "cypress:open": "CYPRESS_baseUrl='http://localhost:4200' CYPRESS_BACKEND_API='http://localhost:8080' cypress open",
    "cypress:run": "CYPRESS_baseUrl='http://localhost:4200' CYPRESS_BACKEND_API='http://localhost:8080' cypress run",
    "postinstall": "npm run build:prod",
    "build:circleci": "ng build --prod --configuration='e2e' --progress=false",
    "cypress:circleci": "CYPRESS_baseUrl='http://localhost:4200' CYPRESS_BACKEND_API='http://localhost:4200' cypress run --record"
  },
  "main": "server/index.js",
  "private": true,
  "engines": {
    "node": "10.9.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/pwa": "^0.10.5",
    "@angular/router": "^7.0.3",
    "@angular/service-worker": "^7.0.3",
    "@types/socket.io-client": "^1.4.32",
    "compression": "^1.7.3",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@types/node": "^10.12.5",
    "codelyzer": "^4.5.0",
    "cypress": "^3.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
